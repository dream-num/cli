{
  "name": "@univerjs/create-cli",
  "type": "module",
  "version": "0.5.2",
  "description": "Command-line tool for quickly playing with Univer.",
  "author": "DreamNum <developer@univer.ai>",
  "license": "Apache-2.0",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "create-cli": "index.js"
  },
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "node ./esbuild.config.mjs --watch",
    "build": "node ./esbuild.config.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "release-it",
    "release:alpha": "release-it preminor --preRelease=alpha",
    "release:beta": "release-it preminor --preRelease=beta",
    "release:rc": "release-it prerelease --preRelease=rc"
  },
  "dependencies": {
    "@inquirer/prompts": "^4.1.0",
    "@univerjs/core": "0.1.0-beta.5",
    "@univerjs/design": "0.1.0-beta.5",
    "@univerjs/docs": "0.1.0-beta.5",
    "@univerjs/docs-ui": "0.1.0-beta.5",
    "@univerjs/engine-formula": "0.1.0-beta.5",
    "@univerjs/engine-numfmt": "0.1.0-beta.5",
    "@univerjs/engine-render": "0.1.0-beta.5",
    "@univerjs/facade": "0.1.0-beta.5",
    "@univerjs/find-replace": "0.1.0-beta.5",
    "@univerjs/network": "0.1.0-beta.5",
    "@univerjs/rpc": "0.1.0-beta.5",
    "@univerjs/sheets": "0.1.0-beta.5",
    "@univerjs/sheets-formula": "0.1.0-beta.5",
    "@univerjs/sheets-numfmt": "0.1.0-beta.5",
    "@univerjs/sheets-ui": "0.1.0-beta.5",
    "@univerjs/sheets-zen-editor": "0.1.0-beta.5",
    "@univerjs/ui": "0.1.0-beta.5",
    "@univerjs/uniscript": "0.1.0-beta.5",
    "@wendellhu/redi": "^0.13.0",
    "clsx": "^2.1.0",
    "commander": "^12.0.0",
    "consola": "^3.2.3",
    "ejs": "^3.1.9",
    "fs-extra": "^11.2.0",
    "os-locale": "^6.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "esbuild": "^0.20.1",
    "esbuild-plugin-clean": "^1.0.1",
    "eslint": "^8.57.0",
    "lint-staged": "^15.2.2",
    "minimist": "^1.2.8",
    "release-it": "^17.1.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
